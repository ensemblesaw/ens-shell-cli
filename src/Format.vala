namespace Ensembles._Console {
    /**
     * Formatting strings for terminal.
     */
    internal class Format {
        /** Color Red */
        public const string RED = "\x1B[31m";
        /** Color Green */
        public const string GRN = "\x1B[32m";
        /** Color Yellow */
        public const string YEL = "\x1B[33m";
        /** Color Blue */
        public const string BLU = "\x1B[34m";
        /** Color Magenta */
        public const string MAG = "\x1B[35m";
        /** Color Cyan */
        public const string CYN = "\x1B[36m";
        /** Color White */
        public const string WHT = "\x1B[37m";
        /** Style Bold */
        public const string BOLD = "\x1B[1m";
        /** Format Reset */
        public const string RESET = "\x1B[0m";
    }
}
